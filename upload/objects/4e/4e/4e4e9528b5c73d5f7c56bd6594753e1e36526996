# Configuration file

general {
    # Enable debug mode
    B:debug=false

    # Base maximum stack size to extract
    # Min: 1
    # Max: 64
    I:extractSize=1

    # Time in ticks between extractions
    # Min: 0
    # Max: 2147483647
    I:extractTime=4

    # Enable help about how to use GUIs
    B:showHelpTooltips=true

    # Super upgraded maximum stack size to extract
    # Min: 1
    # Max: 64
    I:superUpgradedExtractSize=64

    # Time in ticks for an item to travel 1 pipe block
    # Min: 0
    # Max: 2147483647
    I:travelTime=4

    # Time in ticks between pipe network scan
    # increasing will reduce server load, but also responsiveness
    # Min: 0
    # Max: 2147483647
    I:updateInterval=60

    # Upgraded maximum stack size to extract
    # Min: 1
    # Max: 64
    I:upgradedExtractSize=16

    # Upgraded time in ticks between extractions
    # Min: 0
    # Max: 2147483647
    I:upgradedExtractTime=1
}


