
[general]
	#This value determines how much experience is required to add a chunk. Default Value: 0.5
	#Range: 0.0 ~ 1.7976931348623157E308
	expNeededPerChunk = 0.5
	#This value determines which blocks can provide protect power.
	# Template: "item|tag:id of item or tag{nbt};[power]"
	powerProvider = ["tag:minecraft:banners;1", "item:minecraft:yellow_banner{display:{Name:'{\"text\":\"template_territory\"}'}};3"]
	#Only show these permissions in territory table gui.
	#Available value:enter attack_entity break_block place_block use_door use_chest interact_entity use_item_on_block use_table 
	permissionBlackList = ["enter", "attack_entity", "break_block", "place_block", "use_door", "use_chest", "interact_entity", "use_item_on_block", "use_table"]
	#Those permissions that are not in the whitelist will be added to the default permissions list.
	addToDefault = true
	#These permissions will default allow when players add territory, unless players change them manually.
	#Available value:enter attack_entity break_block place_block use_door use_chest interact_entity use_item_on_block use_table 
	defaultPermission = ["enter"]

["default protect"]
	#If set to true, no fire will appear in the territory. Except above Obsidian and NetherRack
	preventFire = true
	#If set to true, prevent explosion damage to blocks.
	preventExplosion = true

